{
  "name": "ball-bounce-frp",
  "version": "0.0.1-alpha",
  "description": "Ball Bounce FRP",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "dev": "npm-run-all -p _devserver",
    "dev:auto-reload": "npm-run-all -p _devserver:auto-reload",
    "dev:public:auto-reload": "npm-run-all -p _devserver:auto-reload:public",
    "_devserver": "cross-env NODE_ENV=dev webpack-dev-server --progress --open --config webpack.dev.js",
    "_devserver:public": "cross-env NODE_ENV=dev BROADCAST=true webpack-dev-server --host 0.0.0.0 --progress --open --config webpack.dev.js",
    "_devserver:auto-reload": "cross-env NODE_ENV=dev-auto-reload webpack-dev-server --progress --open --config webpack.dev.js",
    "_devserver:auto-reload:public": "cross-env NODE_ENV=dev-auto-reload BROADCAST=true webpack-dev-server --host 0.0.0.0 --progress --open --config webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --progress --config webpack.prod.js && cp -R ./src/webpage/static ./dist/ && cp ./src/webpage/_redirects ./dist/",
    "dist:server": "http-server ./dist -o"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/src/app/",
      "<rootDir>/src/"
    ]
  },
  "author": "David Komer",
  "license": "MIT",
  "devDependencies": {
    "@types/gl-matrix": "2.4.0",
    "@types/jest": "23.0.0",
    "@types/node": "10.3.2",
    "chokidar": "2.0.3",
    "clean-webpack-plugin": "0.1.19",
    "cors": "^2.8.4",
    "cross-env": "5.1.6",
    "css-loader": "0.28.11",
    "express": "^4.16.3",
    "fast-sass-loader": "1.4.5",
    "fork-ts-checker-webpack-plugin": "0.4.2",
    "glob": "7.1.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "input-senders": "1.0.2",
    "install": "^0.11.0",
    "jest": "23.1.0",
    "node-sass": "4.9.0",
    "npm": "^6.1.0",
    "npm-run-all": "4.1.3",
    "null-loader": "0.1.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "serve-index": "^1.9.1",
    "shelljs": "0.8.2",
    "source-map-loader": "0.2.3",
    "style-loader": "0.21.0",
    "ts-jest": "22.4.6",
    "ts-loader": "4.4.0",
    "ts-node": "6.1.0",
    "ts-shader-loader": "1.0.5",
    "tsconfig-paths": "3.3.2",
    "typescript": "2.9.1",
    "uglify-js": "3.4.0",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.3",
    "webpack-dev-server": "3.1.4",
    "webpack-merge": "4.1.2",
    "worker-loader": "2.0.0"
  },
  "dependencies": {
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "fluture": "8.0.2",
    "fluture-loaders": "2.1.7",
    "gl-matrix": "^2.6.1",
    "orbit-controls": "^1.2.4",
    "partial.lenses": "13.13.0",
    "pure3d": "0.1.1",
    "ramda": "^0.25.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "sanctuary": "0.15.0",
    "sodiumjs": "1.1.3"
  }
}
